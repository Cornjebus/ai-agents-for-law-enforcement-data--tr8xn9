{
  "name": "autonomous-revenue-platform-backend",
  "version": "1.0.0",
  "description": "Backend services for the Autonomous Revenue Generation Platform",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "concurrently \"ts-node-dev --respawn src/index.ts\" \"python -m uvicorn src.main:app --reload\"",
    "build": "tsc && python -m pip install -r requirements.txt",
    "test": "jest && python -m pytest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage && python -m pytest --cov",
    "lint": "eslint . --ext .ts && pylint src/**/*.py",
    "format": "prettier --write \"src/**/*.ts\" && black src/",
    "migrate": "prisma migrate deploy && alembic upgrade head",
    "seed": "prisma db seed && python src/scripts/seed.py",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "security:audit": "pnpm audit && safety check",
    "celery:worker": "celery -A src.tasks worker --loglevel=info",
    "celery:beat": "celery -A src.tasks beat --loglevel=info",
    "prepare:dev": "husky install && pip install -r requirements.dev.txt"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.4.3",
    "@aws-sdk/client-cloudwatch": "^3.0.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.0.0",
    "@aws-sdk/client-kms": "^3.0.0",
    "@aws-sdk/client-polly": "^3.0.0",
    "@aws-sdk/client-sagemaker-runtime": "^3.0.0",
    "@aws-sdk/client-wafv2": "^3.0.0",
    "@hubspot/api-client": "^9.0.0",
    "@kong/kong-js": "1.5.0",
    "@linkedin/api-client": "^1.0.0",
    "@nestjs/circuit-breaker": "^1.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/throttler": "^5.0.0",
    "@opentelemetry/api": "^1.0.0",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@prisma/client": "^4.16.0",
    "@tensorflow/tfjs-node": "^4.10.0",
    "@tiktok-business/sdk": "^1.0.0",
    "@types/node": "^18.16.0",
    "aws-xray-sdk": "^3.3.0",
    "awilix": "^8.0.1",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.10.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dd-trace": "^3.15.0",
    "dotenv": "^16.3.1",
    "eventemitter2": "^6.4.0",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-openapi-validator": "^5.0.0",
    "express-rate-limit": "^6.7.0",
    "fastapi": "^0.100.0",
    "helmet": "^7.0.0",
    "hot-shots": "^9.3.0",
    "inversify": "^6.0.1",
    "ioredis": "^5.3.0",
    "jose": "^4.14.4",
    "js-yaml": "^4.1.0",
    "jsforce": "^1.11.0",
    "jsonwebtoken": "^9.0.0",
    "kafkajs": "^2.2.4",
    "langchain": "^0.0.27",
    "memory-cache": "^0.2.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "openai": "^4.0.0",
    "openapi3-ts": "^3.0.0",
    "openid-client": "^4.9.1",
    "opossum": "^6.0.0",
    "pg": "^8.11.0",
    "prom-client": "^14.2.0",
    "rate-limiter-flexible": "^2.4.1",
    "redis": "^4.6.8",
    "retry": "^0.13.1",
    "retry-ts": "^3.0.0",
    "samlify": "^2.8.0",
    "sequelize": "^6.32.1",
    "stripe": "^12.0.0",
    "tsyringe": "^4.8.0",
    "twitter-api-v2": "^1.15.0",
    "typeorm": "^0.3.0",
    "uuid": "^9.0.0",
    "validator": "^13.11.0",
    "winston": "^3.9.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@jest/globals": "^29.6.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "autocannon": "^7.11.0",
    "aws-sdk-mock": "^5.8.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "jest-mock": "^29.0.0",
    "k6": "^0.46.0",
    "nock": "^13.3.2",
    "prettier": "^2.8.8",
    "prisma": "^4.16.0",
    "snyk": "1.0+",
    "supertest": "^6.3.3",
    "testcontainers": "^9.0.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "tsoa": "^5.1.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0",
    "python": ">=3.11.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  }
}